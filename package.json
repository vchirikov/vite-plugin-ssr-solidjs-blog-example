{
  "name": "ssg",
  "author": "Vladimir Chirikov",
  "private": true,
  "license": "UNLICENSED",
  "version": "0.0.0-placeholder",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:vite": "cross-env servedUrl=http://localhost:3000/ vite dev",
    "dev:i18n": "typesafe-i18n",
    "build": "vite build",
    "build:prerender": "cross-env NODE_OPTIONS='--no-warnings' node build/prerender.js",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "test": "cross-env NODE_OPTIONS='--no-warnings' vitest run",
    "test:watch": "cross-env NODE_OPTIONS='--no-warnings' vitest",
    "test:ui": "cross-env NODE_OPTIONS='--no-warnings' vitest --ui",
    "test:update": "cross-env NODE_OPTIONS='--no-warnings' vitest --update"
  },
  "imports": {
    "#types": "./src/types/types",
    "#shared/*": "./src/lib/shared/*",
    "#client/*": "./src/lib/client/*",
    "#server/*": "./src/lib/server/*",
    "#src/*": "./src/*",
    "#tests/*": "./tests/*",
    "#root/*": "./*"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^2.3.0",
    "@resvg/resvg-js": "^2.4.1",
    "@rollup/pluginutils": "^5.0.2",
    "@solidjs/testing-library": "^0.6.1",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/mdx": "^2.0.3",
    "@types/node": "^18.14.0",
    "@types/object-hash": "^3.0.2",
    "@types/prismjs": "^1.26.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitest/ui": "^0.28.5",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "daisyui": "^2.50.1",
    "eslint": "^8.34.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-solid": "^0.9.4",
    "eslint-plugin-unicorn": "^45.0.2",
    "happy-dom": "^8.6.0",
    "nerdbank-gitversioning": "3.6.65-alpha",
    "npm-run-all": "^4.1.5",
    "object-hash": "^3.0.0",
    "postcss": "^8.4.21",
    "refractor": "^4.8.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.0.1",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-copy-linked-files": "github:joostdecock/remark-copy-linked-files",
    "remark-gfm": "^3.0.1",
    "rimraf": "^4.1.2",
    "satori": "^0.3.2",
    "satori-html": "^0.3.2",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vfile": "^5.3.7",
    "vfile-matter": "^4.0.1",
    "vite": "^4.1.2",
    "vite-plugin-solid": "^2.5.0",
    "vite-plugin-ssr": "0.4.85",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@solid-primitives/media": "^2.1.0",
    "@solidjs/meta": "^0.28.2",
    "inversify": "^6.0.1",
    "prismjs": "^1.29.0",
    "solid-js": "^1.6.11",
    "solid-jsx": "^0.9.1",
    "typesafe-i18n": "5.24.1",
    "yaml": "^2.2.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}